# Copy this file to `.env` and fill in your secrets.
# Never commit real keys or secrets to source control.

# Firebase (client-side)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Gemini (server-side, used by /api/analyze-video)
# Either variable name works; the serverless function will use the first one it finds.
GOOGLE_GENERATIVE_AI_API_KEY=
GEMINI_API_KEY=

# Gemini (client-side, used directly in the browser)
# WARNING: Exposes key to users. Use only if acceptable for your use case.
VITE_GEMINI_API_KEY=
VITE_GOOGLE_GENERATIVE_AI_API_KEY=
